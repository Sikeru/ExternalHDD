<mapper namespace="adminDAO">
	<select id="selectMainImgList" resultType="hashmap">
		SELECT IMAGE_NM, IMAGE, IMAGE_DC, REFLCT_AT FROM COMTNMAINIMAGE ORDER BY IMAGE_FILE ASC
	</select>
	
	<select id="lottoListCnt" resultType="int">
		SELECT IMAGE_NM, IMAGE, IMAGE_DC, REFLCT_AT FROM COMTNMAINIMAGE ORDER BY IMAGE_FILE ASC
	</select>
	
</mapper>